{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "lib": ["esnext", "dom"],
    "module": "commonjs", //esnext
    "moduleResolution": "node",
    "noEmit": false,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "removeComments": true,
    "sourceMap": true,
    "strict": false,
    "strictFunctionTypes": true,
    "target": "esnext"
  },
  "include": [
    "./bin",
    "./index.ts",
    "temp-elements/removed/handleNonNestedElements.ts",
    "temp-elements/removed/handleNestedElements.ts",
    "temp-elements/removed/getElementType.ts",
    "temp-elements/removed/getDescription.ts"
  ],
  "exclude": ["./templates/*", "./temp-elements"]
}
